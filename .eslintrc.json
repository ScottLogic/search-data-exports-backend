{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "comma-dangle": "off",
    "no-underscore-dangle": "off",
    "no-restricted-syntax": "off",
    "import/no-extraneous-dependencies": ["error", { "ignore": ["aws-sdk"] }],
    "import/prefer-default-export": "off",
    "no-console": "off"
  }
}
