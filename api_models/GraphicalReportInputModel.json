{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "GraphicalReportInputModel",
  "type": "object",
  "required": ["download", "search"],
  "properties": {
    "download": { "type": "boolean" },
    "search": {
      "type": "array",
      "minItems": 0,
      "items": {
        "type": "object",
        "required": ["value"],
        "properties": { "value": { "type": "string" } }
      }
    }
  }
}
